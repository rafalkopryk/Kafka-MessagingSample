{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "EventBus": {
    "bootstrapservers": "localhost:9092",
    "groupid": "messaging",
    "enableautocommit": false,
    "statisticsintervalms": 5000,
    "sessiontimeoutms": 6000,
    "autooffsetreset": "earliest",
    "enablepartitioneof": true
  }
}
